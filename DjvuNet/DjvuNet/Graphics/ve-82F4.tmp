                    }
                    else
                    {
                        for (int x = rect2.xmax - rect2.xmin; x-- > 0; pixel.incOffset(), refPixel.incOffset())
                        {
                            pixel.set_Renamed(ref_Renamed.ramp(refPixel));
                        }
                    }
                }
            }

            return this;
        }

