                }

                rect = pdr;
                destwidth = rect.width();
                destheight = rect.height();
            }

            init(destheight, destwidth, null);

            int sy = rect.ymin / 3;
            int dy = rect.ymin - (3 * sy);

            //    if(dy < 0)
            //    {
            //      sy--;
            //      dy += 3;
