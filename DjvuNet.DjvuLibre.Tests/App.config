<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="xunit.maxParallelThreads" value="1"/>
    <add key="xunit.longRunningTestSeconds" value="120"/>
    <add key="xunit.shadowCopy" value="False"/>
