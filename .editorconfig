; This file is for unifying the coding style for different editors and IDEs.
; More information at http://EditorConfig.org

root = true

[*]
insert_final_newline = true

[*.{cs, c, cpp, h, hpp, json}]
indent_style = space
indent_size = 4

[*.{xml, props, targets, *proj, config}]
indent_style = space
indent_size = 2